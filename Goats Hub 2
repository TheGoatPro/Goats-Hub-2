local Library =
loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Goats Hub 2", "BloodTheme")

---The world sections
local TWOHTab = Window:NewTab("TwOh Stuff")
local twoh1stSection = TWOHTab:NewSection("Tapper")
twoh1stSection:NewButton("OverHeavenKnifeThrow", "ButtonInfo", function()
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
end)
twoh1stSection:NewButton("OverHeavenLightningBolt", "ButtonInfo", function()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
end)
local twoh2ndSection = TWOHTab:NewSection("Continuous")
twoh2ndSection:NewToggle("HeavenlyLightningBolt", "ToggleInfo", function(state)
    if state then
        _G.thunder = true
while _G.thunder == true do
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
wait()
end
    else
        _G.thunder = false
while _G.thunder == true do
game:GetService("ReplicatedStorage").SpecialMoves.HeavenlyLightningStrike:FireServer()
wait()
end
    end
end)
twoh2ndSection:NewToggle("HeavenlyKnife", "ToggleInfo", function(state)
    if state then
        _G.thunder = true
while _G.thunder == true do
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
wait()
end
    else
        _G.thunder = false
while _G.thunder == true do
game:GetService("ReplicatedStorage").Attacks.OverHeavenKnifeThrow:FireServer()
wait()
end
    end
end)
local twoh3rdSection = TWOHTab:NewSection("Inf Heal")
twoh3rdSection:NewToggle("HeavenlyHeal", "Heals LocalPlayer", function(state)
    if state then
        _G.twoh = true
while _G.twoh == true do
local args = {
    [1] = true,
    [2] = game:GetService("Players").LocalPlayer.Character.Humanoid,
    [3] = math.huge,
    [7] = 1,
    [8] = "rbxassetid://1202656211",
    [9] = 2.25
}

game:GetService("ReplicatedStorage").Attacks.Heal.OverHeavenHeal:FireServer(unpack(args))
wait()
end
    else
        _G.twoh = false
while _G.twoh == true do
local args = {
    [1] = true,
    [2] = game:GetService("Players").LocalPlayer.Character.Humanoid,
    [3] = math.huge,
    [7] = 0.001,
    [8] = "rbxassetid://1202656211",
    [9] = 2.5
}

game:GetService("ReplicatedStorage").Attacks.Heal.OverHeavenHeal:FireServer(unpack(args))
wait()
end
    end
end)
twoh3rdSection:NewToggle("Heal EveryOne", " ", function(state)
 if state then
getgenv().Toggled =false
if state then
getgenv().Delay = 0
local UIS=game:GetService'UserInputService'
local function Source()
local LP = game.Players.LocalPlayer
local success, err = pcall(function()
for i,v in pairs(game:GetService("Players"):GetChildren()) do
if v ~= game.Players.LocalPlayer then

local args = {
    [1] = false,
    [2] = v.Character.Humanoid,
    [3] = math.huge,
    [4] = v.Character.HumanoidRootPart.Position,
    [7] = 0.001,
    [8] = "rbxassetid://1202656211",
    [9] = 2.5
}

game:GetService("ReplicatedStorage").Attacks.Heal.OverHeavenHeal:FireServer(unpack(args))
wait(0.1)
end
end
end)
if err then
warn(err)
end
end
    if state then
        if getgenv().Toggled then
            getgenv().Toggled=false
                    elseif not getgenv().Toggled then
            getgenv().Toggled=true
            while getgenv().Toggled do
                Source()
                wait(getgenv().Delay)
            end
        end
    end
end
    else
       getgenv().Toggled=false
    end
end)
---end of the world over heaven section








